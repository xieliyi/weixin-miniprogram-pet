<!--main.wxml-->

<view class="userinfo" style='display: flex;flex-direction: column;align-items: center;'>
  <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" mode="cover"></image>
  <text class="userinfo-nickname">{{userInfo.nickName}}</text>
</view>

<form bindsubmit="formSubmit" bindreset="formReset">

  <view style="background-size:contain;background-image: url('data:image/jpg;base64,');">

  <view class="section">
    <view class="section__title">SN序列号</view>
    <input class="section__text" name="rSN" type='text' placeholder="0123456789" value='{{snNum}}' maxlength='10' disabled='true'/>
  </view>

  <view class="section">
    <view class="section__title">萌宠昵称</view>
    <input class="section__text" name="rName" type='text' placeholder="萌宠昵称" value='{{userData.name}}' maxlength='10'/>
  </view>

  <view class="section">
    <view style="display:flex;flex-direction:row;">
    <view class="section__title">主人电话</view>
    <view style='background-color: #fff;'>
      <text style='color:red; font-size: 26rpx;margin-left: 0rpx;margin-bottom: 0rpx;background-color: #fff;'>（必填）</text>
    </view>
    </view>
    <input class="section__text" name="rMobile" type='number' placeholder="主人电话" value='{{userData.mobile}}' maxlength='11' />
  </view>

  <view class="section">
    <view class="section__title">地址</view>
    <textarea class="section__text_addr" name="rAddress" type='text' placeholder="地址" value='{{userData.address}}' maxlength='35' />
  </view>

  </view>

  <view class="buttonary">
    <block wx:if="{{!registered}}">
      <button class='reg-button' type='default' disabled='{{registered}}' style='align-items: center' formType="submit">  保存  </button>
    </block>
    <block wx:else>
      <button class='reg-button' type='default' disabled='{{!registered}}' style='align-items: center' formType="reset">  删除  </button>
    </block>
  </view>

</form>
